<div class="container">
  <div class="row">
    <div class="col-md-10 ">
        <table class="table table-striped table-bordered table-list">
          <thead>
            <tr>
              <th>用户名</th>
              <th>付费类型</th>
              <th>付费方式</th>
              <th>有效期</th>
              <th>价格</th>
              <th>订单创建时间</th>
            </tr>
          </thead>

          <% @users.each do |user| %>
          <% user.orders.each do |order| %>

         <tbody>
          <td><%= link_to(user.email, admin_user_path(user)) %></td>
          <td><%= order.order_type %></td>
          <td><%= order.payment_method %></td>
          <td><%= order.subscription_months %> months</td>

          <td>￥ <%= order.price %></td>
          <td><%= render_formatted_time(order.created_at)  %></td>
        </tbody>
        <% end %>
        <% end %>
      </table>

    </div>
   </div>
  </div>
